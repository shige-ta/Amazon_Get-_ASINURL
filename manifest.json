{
  "manifest_version": 2,
  "name": "Amazon Get ASINURL",
  "version": "1.0.0",
  "description": "AmazonのASINURL取得するChrome拡張です",
  "content_scripts": [
    {
      "matches": ["https://www.amazon.co.jp/*"],
      "css":["style.css"]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}